{
    "openapi": "3.1",
    "servers": [
        {
            "url": "https://api.fieldobservatory.org"
        }
    ],
    "info": {
        "title": "Farm management data OpenAPI documentation",
        "description": "There is no API. This page is only for testing the OpenAPI compatibility of farm management data schemas.",
        "version": "0.0.0"
    },
    "paths": {
        "/management/{site}": {
            "get": {
                "operationId": "management",
                "parameters": [
                    {
                        "in": "path",
                        "name": "site",
                        "description": "site ID",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "404": {
                        "description": "not found"
                    },
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "management-data.schema.json"
                                }
                            }
                        },
                        "description": "response"
                    }
                }
            }
        }
    }
}